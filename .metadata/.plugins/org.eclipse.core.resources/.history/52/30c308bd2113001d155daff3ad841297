public class dipTest {

	public static void main(String[] args) {
	DBLogger db = new DBLogger();
		TaxCalculator t = new TaxCalculator(db);
		System.out.println(t.calculateTax(20, 10));
		System.out.println(t.calculateTax(10, 0));
	}

}
